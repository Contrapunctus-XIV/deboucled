{
  "env": {
    "browser": true,
    "es2021": true,
    "greasemonkey": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "ignorePatterns": ["variables.js", "storage.js", "extensions.js", "preboucles.js", "stats.js", "topics.js", "messages.js", "settings.js", "privatemessages.js", "main.js"],
  "rules": {
    "no-unused-vars": "warn"
  }
}
